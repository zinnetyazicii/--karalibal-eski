{
  "comparePageTableStyle": {
    "type": "Variable",
    "variable": "table",
    "selector_prefix": ".route-product-compare #content"
  },
  "comparePageCartButtonStyle": {
    "type": "Variable",
    "variable": "button",
    "selector_prefix": ".route-product-compare .compare-buttons .btn-cart&"
  },
  "comparePageRemoveButtonStyle": {
    "type": "Variable",
    "variable": "button",
    "selector_prefix": ".route-product-compare .compare-buttons .btn-remove&"
  },
  "comparePageCartButtonVisibility": {
    "type": "Toggle",
    "rules": {
      "true": {
        ".route-product-compare .compare-buttons .btn-cart": "display: flex"
      },
      "false": {
        ".route-product-compare .compare-buttons .btn-cart": "display: none"
      }
    }
  },
  "comparePageRemoveButtonVisibility": {
    "type": "Toggle",
    "rules": {
      "true": {
        ".route-product-compare .compare-buttons .btn-remove": "display: flex"
      },
      "false": {
        ".route-product-compare .compare-buttons .btn-remove": "display: none"
      }
    }
  },

  "comparePageName": {
    "type": "Toggle",
    "rules": {
      "true": {
        ".compare-name": "display: table-row"
      },
      "false": {
        ".compare-name": "display: none"
      }
    }
  },
  "comparePageImage": {
