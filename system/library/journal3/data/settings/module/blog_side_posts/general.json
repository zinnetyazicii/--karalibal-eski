{
  "name": {
    "type": "Input",
    "value": "New Blog Posts "
  },
  "status": {
    "type": "Status"
  },
  "schedule": {
    "type": "Range",
    "php": true
  },
  "moduleTitle": {
    "type": "Variable",
    "variable": "title",
    "selector_prefix": ".module-blog_side_posts-%s .title.module-title"
  },
  "sectionsDisplay": {
    "type": "Radio",
    "value": "blocks",
    "php": true,
    "device": true
  },
  "sectionsDisplayTablet": {
    "type": "Radio",
    "value": "",
    "php": true
  },
  "sectionsDisplayPhone": {
    "type": "Radio",
    "value": "",
    "php": true
  },
  "carousel": {
    "type": "Toggle",
    "value": "false",
    "php": true,
    "device": true
  },
  "carouselTablet": {
    "type": "Toggle",
    "value": "",
    "php": true
  },
  "carouselPhone": {
    "type": "Toggle",
    "value": "",
    "php": true
  },
  "carouselStyle": {
    "type": "Variable",
    "variable": "carousel",
    "selector_prefix": ".module-blog_side_posts-%s "
  },
  "accordionStyle": {
    "type": "Variable",
    "variable": "accordion",
    "selector_prefix": ".module-blog_side_posts-%s "
  },
  "tabsStyle": {
    "type": "Variable",
    "variable": "tabs",
    "selector_prefix": ".module-blog_side_posts-%s "
  },
  "itemsPerRow": {
    "type": "ItemsPerRow",
    "selector": ".module-blog_side_posts-%s .post-layout",
    "php": true
  },
  "default": {
    "type": "Select",
    "value": "",
    "php": true
  },
  "carouselGutter": {
    "type": "Padding",
    "selector": ".module-blog_side_posts-%s .swiper-container"
  },
  "containerMargin": {
    "type": "Margin",
    "selector": ".module-blog_side_posts-%s:not(.carousel-mode) .side-posts"
  },
  "imageDimensions": {
    "type": "ImageDimensions",
    "value": {
      "width": "50",
      "height": "50",
      "resize": "fill"
    },
    "php": true
  },
  "sidePostsStyle": {
    "type": "Variable",
    "variable": "side_posts",
    "selector_prefix": ".module-blog_side_posts-%s"
  }
}
