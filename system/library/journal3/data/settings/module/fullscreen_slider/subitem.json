  },

  "vidOffset": {
    "type": "InputPair",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-video",
    "properties": {
      "first": "margin-left:%s% !important",
      "second": "margin-top:%s% !important"
    }
  },


  "textWidth": {
    "type": "Input",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-caption",
    "property": "width:%s%"
  },
  "textFont": {
    "type": "Font",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-caption"
  },
  "textIcon": {
    "type": "Icon",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-text::before"
