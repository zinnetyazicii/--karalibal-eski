      "first": "margin-left:%s% !important",
      "second": "margin-top:%s% !important"
    }
  },

  "shapeOffset": {
    "type": "InputPair",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-shape",
    "properties": {
      "first": "margin-left:%s% !important",
      "second": "margin-top:%s% !important"
