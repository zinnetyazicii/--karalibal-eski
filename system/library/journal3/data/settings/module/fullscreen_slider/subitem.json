{
  "name": {
    "type": "Input",
    "value": "New Layer"
  },
  "status": {
    "type": "Toggle"
  },
  "resize": {
    "type": "Toggle"
  },
  "type": {
    "type": "Radio",
    "value": "text",
    "php": true
  },
  "text": {
    "type": "InputLang",
    "php": true
  },
  "image": {
    "type": "ImageLang"
  },
  "alt": {
    "type": "InputLang",
    "php": true
  },
  "videoType": {
    "type": "Radio",
    "value": "html5",
    "php": true
  },
  "layerAutoplay": {
    "type": "Toggle",
    "value": "false"
  },
  "videoHtml5Url": {
    "type": "Input"
  },
  "videoYoutubeUrl": {
    "type": "Input"
  },
  "videoVimeoUrl": {
    "type": "Input",
    "value": "top"
  },
  "hotspotAlign": {
    "type": "Radio"
  },

  "hotspotIcon": {
    "type": "Icon",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-hotspot .ms-point-center::before"
  },
  "hotspotIconHover": {
    "type": "Color",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-hotspot .ms-point-center:hover::before",
    "property": "color"
  },

  "hotspotDimensions": {
    "type": "InputSlider",
    "value": "20",
    "rules": {
      "@": {
        ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-hotspot .ms-point-center": "width:%spx; height:%spx"
      }
    }
  },
  "hotspotBorder": {
    "type": "Border",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-hotspot .ms-point-center"
  },
  "hotspotBorderHover": {
    "type": "Color",
    "selector": ".desktop .module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-hotspot .ms-point-center:hover",
    "property": "border-color"
  },
  "hotspotBorderRadius": {
    "type": "BorderRadius",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-hotspot .ms-point-center"
  },
  "hotspotBG": {
    "type": "Background",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-hotspot .ms-point-center"
  },
  "hotspotBGHover": {
    "type": "Background",
    "selector": ".desktop .module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-hotspot .ms-point-center:hover"
  },
  "hotspotShadow": {
    "type": "Shadow",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-hotspot .ms-point-center"
  },
  "hotspotShadowHover": {
    "type": "Shadow",
    "selector": ".desktop .module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-hotspot .ms-point-center:hover"
  },


  "tooltipFont": {
    "type": "Font",
    "selector": ".module-fullscreen_slider-%s .ms-tooltip-cont"
  },
  "tooltipBG": {
    "type": "Background",
    "selector": ".module-fullscreen_slider-%s .ms-tooltip-cont"
  },
  "tooltipTriangle": {
    "type": "Color",
    "rules": {
      "@": {
        ".module-fullscreen_slider-%s .ms-tooltip-top .ms-tooltip-arrow": "border-top-color:%s",
        ".module-fullscreen_slider-%s .ms-tooltip-right .ms-tooltip-arrow": "border-right-color:%s",
        ".module-fullscreen_slider-%s .ms-tooltip-bottom .ms-tooltip-arrow": "border-bottom-color:%s",
        ".module-fullscreen_slider-%s .ms-tooltip-left .ms-tooltip-arrow": "border-left-color:%s"
      }
    }
  },
  "tooltipOffset": {
    "type": "InputPair",
    "selector": ".module-fullscreen_slider-%s .ms-tooltip",
    "properties": {
      "first": "margin-left:%spx",
      "second": "margin-top:%spx"
    }
  },
  "tooltipBorder": {
    "type": "Border",
    "selector": ".module-fullscreen_slider-%s .ms-tooltip-cont"
  },
  "tooltipShadow": {
    "type": "Shadow",
    "selector": ".module-fullscreen_slider-%s .ms-tooltip-cont"
  },
  "tooltipBorderRadius": {
    "type": "BorderRadius",
    "selector": ".module-fullscreen_slider-%s .ms-tooltip-cont"
  },
  "tooltipPadding": {
    "type": "Padding",
    "selector": ".module-fullscreen_slider-%s .ms-tooltip-cont"
  },


  "layerVisibility": {
    "type": "Toggle",
    "value": "true",
    "rules": {
      "true": {
        ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s": "display: inline-flex; visibility:visible !important"
      },
      "false": {
        ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s": "display: none; visibility:hidden !important"
      }
    }
  },

  "textOffset": {
    "type": "InputPair",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-text",
    "properties": {
      "first": "margin-left:%s% !important",
      "second": "margin-top:%s% !important"
    }
  },

  "btnOffset": {
    "type": "InputPair",
    "selector": ".module-fullscreen_slider-%s .module-item-%s .module-subitem-%s.ms-layer-button",
    "properties": {
      "first": "margin-left:%s% !important",
      "second": "margin-top:%s% !important"
