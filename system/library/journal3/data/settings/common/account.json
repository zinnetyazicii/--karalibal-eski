  "Display": {
    "type": "Radio",
    "value": "links",
    "rules": {
      "links": {
        ".account-list": "flex-direction:column",
        ".account-list > li": "margin-bottom: 5px; padding:0; width:100%",
        ".account-list > li > a": "flex-direction: row; width: auto; text-align: left;",
        ".account-list > li > a::before": "margin-right:5px"
      },
      "boxes": {
        ".account-list": "flex-direction:row",
        ".account-list > li": "margin: 0",
        ".account-list > li > a": "flex-direction: column; width: 100%; text-align: center;",
