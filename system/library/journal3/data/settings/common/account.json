  "TitlesStyle": {
    "type": "Variable",
    "variable": "title",
    "selector_prefix": ".account-page .title"
  },
  "Boxes": {
    "include": "common_hover",
    "selector_prefix": ".account-list > li > a"
  },
  "BoxPerRow": {
    "type": "InputNumber",
    "value": "6",
    "rules":{
      "@":{
        ".account-list > li": "width:calc(100% / %s)"
      }
    }
  },
  "BoxSpacing": {
    "type": "InputNumber",
    "value": "8",
    "rules":{
      "@":{
        ".account-list": "margin:0 -%spx -%spx",
        ".account-list > li": "padding:%spx"
      }
    }
  },

